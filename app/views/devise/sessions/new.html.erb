<div id="login" class="container-fluid">

<%= bootstrap_form_for(resource, as: resource_name, url: session_path(resource_name), html: {class: "form-signin", role: "form"}) do |f| %>
      
    <h2 class="form-signin-heading">Welcome back, Bennette.</h2>   
    
      <%= f.email_field :email, class: "form-control", html: {spellcheck: "false", placeholder: "Email address"} %>
  
    
    
      <%= f.password_field :password, class: "form-control", html: {autocomplete: "off", placeholder: "Password", type: "password"} %>
   

                                                                                                                                        
  <% if devise_mapping.rememberable? -%>
      <label class="checkbox">
        <%= f.check_box :remember_me %>
      </label>
  <% end -%>
                                                                                                                                        
  
    
      <%= f.submit "Login", class: "btn btn-lg btn-primary btn-block" %>
  <%= render "devise/shared/links" %>
  <% end %>
</div>
